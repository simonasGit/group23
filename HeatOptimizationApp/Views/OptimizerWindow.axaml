<Window
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.Avalonia;assembly=LiveChartsCore.SkiaSharpView.Avalonia"
    xmlns:vm="clr-namespace:HeatOptimizationApp.Views"
    x:Class="HeatOptimizationApp.Views.OptimizerWindow"
    x:DataType="vm:OptimizerWindow"
    Title="OptimizerWindow" Width="900" Height="600">

    <Grid>
        <lvc:CartesianChart Series="{Binding Series}" XAxes="{Binding XAxes}" YAxes="{Binding YAxes}" />
    </Grid>
</Window>
